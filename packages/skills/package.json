{
  "name": "@qwen-tools/skills",
  "version": "0.1.0",
  "description": "AST-grep pattern skills for Qwen3-Coder adaptation",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./patterns": {
      "import": "./dist/patterns/index.js",
      "types": "./dist/patterns/index.d.ts"
    }
  },
  "files": [
    "dist",
    "patterns"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ast-grep",
    "patterns",
    "skills",
    "code-analysis",
    "qwen"
  ],
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.0"
  }
}
